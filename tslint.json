{
    "extends": [
        "tslint:latest",
        "tslint-config-airbnb",
        "tslint-config-prettier"
    ],
    "rules": {
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "variable-declaration",
            "property-declaration",
            "member-variable-declaration",
            "arrow-parameter"
        ]
    }
}
