    "author": "Jan Philip Schellenberg",
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "babel-jest": "^24.7.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^4.2.0",
        "eslint-plugin-import": "^2.17.2",
        "express": "^4.16.4",
        "jest": "^24.7.1"
    },
    "main": "./src/index.js"
